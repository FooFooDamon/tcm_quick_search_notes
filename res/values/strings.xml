    <string name="sql_create_test_table">
        CREATE TABLE `pretest` (
            `test_id`   INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE,
            `test_name` TEXT NOT NULL UNIQUE,
            `test_remarks`  TEXT)
    </string>

    <string name="sql_make_test_data">
        insert into pretest(test_name, test_remarks)
        values(?, ?)
    </string>

    <string name="sql_query_test_data">
        select test_id, test_name, test_remarks
        from pretest
        where test_id between 1 and ?
    </string>

    <string name="sql_create_reference_material_table">
        CREATE TABLE `reference_material` (
            `rid`   INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT UNIQUE,
            `name`  TEXT NOT NULL,
            `version`   TEXT DEFAULT \'第1版\',
            `original_authors`  TEXT,
            `editors`   TEXT,
            `issuing_source`    TEXT,
            `issuing_date`  TEXT,
            `remarks`   TEXT
        )
    </string>

    <string name="sql_add_reference_material">
        insert into reference_material(name, version, original_authors, editors,
            issuing_source, issuing_date, remarks)
        values(?, ?, ?, ?,
            ?, ?, ?)
    </string>

    <string name="sql_query_reference_material_details_by_id">
        select name, version,
            original_authors, editors,
            issuing_source, issuing_date,
            remarks
        from reference_material
        where rid = ?
    </string>

    <string name="sql_query_forward_near_reference_material_items" formatted="false">
        select rid as iid, name
        from reference_material
        where iid &lt; ?
        order by iid desc
        limit ?
    </string>

    <string name="sql_query_afterward_near_reference_material_items" formatted="false">
